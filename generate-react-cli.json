{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "styles.tsx",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "customTemplates": {
        "style": "templates/component/style.tsx",
        "component": "templates/component/component.tsx",
        "index": "templates/component/index.tsx"
      },
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false
    },
    "page": {
      "path": "src/pages",
      "withLazy": true,
      "withStory": false,
      "withStyle": true,
      "withTest": true
    },
    "layout": {
      "path": "src/layout",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": true
    }
  }
}
